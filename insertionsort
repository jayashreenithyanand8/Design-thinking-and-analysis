#include <iostream>
using namespace std;

int main()
{
    int arr[] = {8, 3, 5, 2, 9};
    int n = 5;

    // Insertion Sort
    for(int i = 1; i < n; i++)
    {
        int key = arr[i];      // element to insert
        int j = i - 1;

        // move elements greater than key to one position ahead
        while(j >= 0 && arr[j] > key)
        {
            arr[j + 1] = arr[j];
            j = j - 1;
        }

        arr[j + 1] = key;  // place key at correct position
    }

    // print sorted array
    cout << "Sorted array: ";
    for(int i = 0; i < n; i++)
    {
        cout << arr[i] << " ";
    }

    return 0;
}
